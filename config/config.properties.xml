<properties>
<comment>MapGen config</comment>

<!-- Lineageクライアントのマップディレクトリを指定する。 -->
<entry key="LineageMapDirectory">Z:/L1Tools/MapGen/data/</entry>

<!-- マップファイル出力ディレクトリを指定する。 -->
<entry key="OutputDirectory">Z:/L1Tools/MapGen/output/</entry>

<!-- V1テキストマップファイルを出力する。 yes|no -->
<entry key="OutputV1Maps">yes</entry>

<!-- V2マップファイルを出力する(テスト中)。 yes|no -->
<entry key="OutputV2Maps">yes</entry>

<!-- TextMapReader用のMAP_INFO定義JAVAコードを出力する。 yes|no -->
<entry key="OutputMapInfo">yes</entry>

<!-- map_idsテーブルのCSVデータを出力する。 yes|no -->
<entry key="OutputMapIds">yes</entry>

<!-- map_ids.csvを出力する際に使用するzoneファイルのパス -->
<entry key="ZoneFilePath">Z:/L1Tools/MapGen/config/zone_en_US.txt</entry>

<!-- マップタイルの全値をリストアップし、出力する。テスト用。 yes|no -->
<entry key="OutputTileValue">yes</entry>

</properties>
